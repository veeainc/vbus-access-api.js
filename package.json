{
  "name": "@veea/vbus-access",
  "version": "2.0.0",
  "description": "The Veea Vbus Access Api.",
  "main": "dist/index.js",
  "module": "dist/vbus-access.esm.js",
  "jsnext:main": "dist/vbus-access.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "run-s clean compile bundle",
    "prepare": "npm run build",
    "bundle": "rollup -c",
    "check:format": "prettier --list-different \"**/*.{js,ts,tsx}\"",
    "check:types": "tsc --noEmit",
    "clean": "run-p clean:*",
    "clean:compiled": "del compiled",
    "clean:coverage": "del coverage",
    "clean:dist": "del dist",
    "compile": "tsc -p tsconfig.base.json",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint --ext js,ts,tsx .",
    "postbundle": "del compiled && cp-cli ./index.js ./dist/index.js",
    "version": "run-s test changelog authors && git add .",
    "doc": "typedoc --out docs src"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "@types/json-schema": "^7.0.5",
    "axios": "^0.21.1",
    "jwt-decode": "^2.2.0",
    "universal-cookie": "latest"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@types/jwt-decode": "^2.2.1",
    "better-docs": "^2.0.1",
    "cp-cli": "^2.0.0",
    "cross-env": "^7.0.2",
    "del-cli": "^3.0.1",
    "eslint": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.13.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^9.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.3"
  }
}
